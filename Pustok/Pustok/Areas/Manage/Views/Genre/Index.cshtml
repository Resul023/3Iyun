
@{
    ViewData["Title"] = "Index";
    int count = 0;
}
@model GenreViewModel
<container>
    <row>
        <div class="col-md-10 mx-auto">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">No</th>
                        <th scope="col">Name</th>
                        


                    </tr>
                </thead>
                <tbody>

                    @for (int i = 0; i < Model.Genres.Count; i++)
                    {
                        count++;
                           <tr>
                               <th scope="row">@count</th>
                               <td>@Model.Genres[i].Name</td>
 
                           </tr>

                    }


                </tbody>
            </table>
        </div>
    </row>
</container>

